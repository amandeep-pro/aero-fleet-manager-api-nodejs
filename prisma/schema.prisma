datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String @id @default(uuid()) @db.Uuid
  name  String
  email String @unique
}

model Aircraft {
  id           String   @id @default(uuid()) @db.Uuid
  model        String
  registration String   @unique
  manufacturer String
  capacity     Int
  status       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
